[alias]
	co = checkout
	s = status
	wdiff = diff -w
	set-upstream = !git branch --set-upstream-to=origin/`git symbolic-ref --short HEAD`
    branches = for-each-ref --sort=-committerdate --format=\"%(color:blue)%(authordate:relative)\t%(color:red)%(authorname)\t%(color:white)%(color:bold)%(refname:short)\" refs/remotes --count=20 
